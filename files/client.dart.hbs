  factory FireboomClient.createClient(BaseClientOptions? config) {
    return FireboomClient(BaseClientOptions(
      baseURL: config?.baseURL ?? "{{baseURL}}",
      csrfEnabled: {{isNotEmpty authProviders}}
      ));
  }
}

